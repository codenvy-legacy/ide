<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="css" type="com.codenvy.ide.ui.window.Window.Css" />
  <g:HTMLPanel ui:field="popup" styleName="{css.popup}">
    <g:HTMLPanel ui:field="glass" styleName="{css.glass}"></g:HTMLPanel>
    <g:HTMLPanel styleName="{css.positioner}">
      <g:HTMLPanel ui:field="contentContainer" styleName="{css.content}">
          <g:HTMLPanel ui:field="header" styleName="{css.header}">
              <g:HTMLPanel styleName="{css.headerTitleWrapper}">
                  <g:Label ui:field="headerLabel" addStyleNames="{css.headerTitleLabel}"></g:Label>
              </g:HTMLPanel>
              <g:HTMLPanel ui:field="crossButton" styleName="{css.crossButton}"></g:HTMLPanel>
          </g:HTMLPanel>
          <g:HTMLPanel ui:field="content"></g:HTMLPanel>
          <g:HTMLPanel ui:field="footer" styleName="{css.footer}">
              <g:FlowPanel styleName="{css.separator}"></g:FlowPanel>
          </g:HTMLPanel>
      </g:HTMLPanel>
    </g:HTMLPanel>
  </g:HTMLPanel>
</ui:UiBinder>