<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.codenvy.ide.ui'>
    <ui:with field='locale' type='com.codenvy.ide.ext.cloudbees.client.CloudBeesLocalizationConstant'/>
    <ui:with field='res' type='com.codenvy.ide.ext.cloudbees.client.CloudBeesResources'/>
    <ui:style>
        .alignRight {
            float: right;
        }

        .emptyBorder {
            margin: 6px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="480px" height="240px"
                       addStyleNames="{res.cloudBeesCSS.login} {style.emptyBorder} {res.cloudBeesCSS.appInfo}">
        <g:south size="26.0">
            <g:FlowPanel>
                <c:Button ui:field="btnOk" text="{locale.okButton}" size="SMALL" width="50px" type="SUCCESS"
                          addStyleNames="{style.alignRight}"/>
            </g:FlowPanel>
        </g:south>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridId}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="id" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{res.cloudBeesCSS.event}">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridTitle}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="title" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridServerPool}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="serverpool" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{res.cloudBeesCSS.event}">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridStatus}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="status" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridContainer}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="container" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{res.cloudBeesCSS.event}">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridIdleTimeout}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="idletimeout" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridMaxMemory}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="maxmemory" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{res.cloudBeesCSS.event}">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridSecurityMode}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="securitymode" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridClusterSize}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="clustersize" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="20.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{res.cloudBeesCSS.event}">
                <g:west size="120.0">
                    <g:Label text="{locale.applicationInfoListGridUrl}" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:west>
                <g:center>
                    <g:Label ui:field="url" addStyleNames="{res.cloudBeesCSS.loginFont}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>